                  <paintTicks value="true"/>
                  <preferredSize width="300" height="47"/>
                  <snapToTicks value="true"/>
                  <value value="5"/>
                </properties>
                <clientProperties>
                  <Slider.paintThumbArrowShape class="java.lang.Boolean" value="true"/>
                </clientProperties>
              </component>
              <hspacer id="33481">
                <constraints/>
              </hspacer>
              <grid id="4974e" binding="clockPanel" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                <margin top="0" left="10" bottom="0" right="20"/>
                <constraints/>
