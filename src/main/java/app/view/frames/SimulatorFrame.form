                <properties>
                  <font name="Bradley Hand ITC" style="1"/>
                  <majorTickSpacing value="5"/>
                  <maximum value="50"/>
                  <maximumSize width="300" height="47"/>
                  <minimum value="0"/>
                  <minorTickSpacing value="1"/>
